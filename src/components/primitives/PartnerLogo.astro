---
export interface Props {
  href: string
  logo: string
  alt: string
}
const { href, logo, alt } = Astro.props
---

<a
  class="flex items-center justify-start h-14 w-60 md:h-28 max-w-full mx-auto md:mx-0 md:justify-center"
  href={href}
  target="_blank"
  rel="noopener noreferrer"
>
  <img src={logo} alt={alt} class="max-h-full max-w-full object-contain" />
</a>
